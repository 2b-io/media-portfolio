{{#> layouts/default}}
  {{#*inline "content-body"}}
    <div class="container container-full-screen">
      <div class="container-center">
        {{> partials/header.hbs}}
        <div class="header-bottom">
          <p class="header-text">
            {{{labels.header.text}}}
          </p>
            <a class="lean-more" href="/" target="_blank">{{labels.header.link_title}}</a>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="feature container-center">
        <div class="container-center-box">
          <div class="feature-info">
            <div class="feature-box-img-wraper">
              <img class="feature-box-img" src={{assets.feature_icon.totally_optimized}} alt="optimized">
            </div>
            <div class="feature-info-box">
              <p class="feature-info-box-title">{{labels.feature.totally_optimized.title}}</p>
              <p class="feature-info-box-label">{{labels.feature.totally_optimized.text}}</p>
              <a class="feature-info-box-link" href="#" target="_blank">{{labels.feature.totally_optimized.footer_text}}</a>
            </div>
          </div>
          <div class="feature-info">
            <div class="feature-box-img-wraper">
              <img class="feature-box-img" src={{assets.feature_icon.powerful_features}} alt="features">
            </div>
            <div class="feature-info-box">
              <p class="feature-info-box-title">{{labels.feature.powerful_features.title}}</p>
              <p class="feature-info-box-label">{{labels.feature.powerful_features.text}}</p>
              <a class="feature-info-box-link" href="#" target="_blank">{{labels.feature.powerful_features.footer_text}}</a>
            </div>
          </div>
        </div>
        <div class="container-center-box">
          <div class="feature-info">
            <div class="feature-box-img-wraper">
              <img class="feature-box-img" src={{assets.feature_icon.global_network}} alt="network">
            </div>
            <div class="feature-info-box">
              <p class="feature-info-box-title">{{labels.feature.global_network.title}}</p>
              <p class="feature-info-box-label">{{labels.feature.global_network.text}}</p>
              <a class="feature-info-box-link" href="#" target="_blank">{{labels.feature.global_network.footer_text}}</a>
            </div>
          </div>
          <div class="feature-info">
            <div class="feature-box-img-wraper">
              <img class="feature-box-img" src={{assets.feature_icon.totally_saved}} alt="saved">
            </div>
            <div class="feature-info-box">
              <p class="feature-info-box-title">{{labels.feature.totally_saved.title}}</p>
              <p class="feature-info-box-label">{{labels.feature.totally_saved.text}}</p>
              <a class="feature-info-box-link" href="#" target="_blank">{{labels.feature.totally_saved.footer_text}}</a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="maps container">
      <div class="container-center">
        <div class="maps-label">
          <p class="maps-p">{{{labels.maps.label}}}</p>
        </div>
        <div class="maps-background-wraper">
        </div>
        <div class="maps-footer">
          {{#each labels.maps.title}}
            <div class="maps-footer-text">
              <p class="maps-footer-p">
                <span class="maps-span-value">{{{this.value}}}</span><br/><br/>
                <span class="maps-span-small">{{this.text}}<span>
              </p>
            </div>
          {{/each}}
        </div>
      </div>
    </div>
    <div class="container">
      <div class="feature container-center">
        <div class="what-new what-new-title">
          <p>{{labels.what_new.title}}</p>
        </div>
        <div class="what-new what-new-image-wraper">
          {{#each labels.what_new.feature}}
            <div class="what-new-image-box">
              <img class="what-new-image" src={{this.src}} alt="{{this.alt}}">
              <p class="what-new-image-detail">{{this.detail}}</p>
            </div>
          {{/each}}
        </div>
      </div>
    </div>

    {{> partials/subscribe.hbs}}

    {{> partials/collaborator.hbs}}

    {{> partials/footer.hbs}}
  {{/inline}}
{{/ layouts/default}}
