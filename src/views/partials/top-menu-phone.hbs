<div class="menu-mobile">
  <input type="checkbox" id="menu-mobile-checkbox">
  <label class="menu-mobile-label" for="menu-mobile-checkbox">
    <div class="toggle-button"></div>
  </label>
  <label class="menu-mobile-hidden" for="menu-mobile-checkbox">
  </label>
  <div class="menu-mobile-wraper">
    <ul class="menu-mobile-wraper-ul">
      {{#each urls.menu}}
        <li class="menu-item">
          <a class="menu-item-a" href="{{this.link}}">{{this.label}}</a>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
